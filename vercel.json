{
  "framework": null,
  "buildCommand": "",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    },
    {
      "source": "/styles.css",
      "headers": [
        { "key": "Cache-Control", "value": "public, s-maxage=86400, max-age=0, stale-while-revalidate=86400" }
      ]
    },
    {
      "source": "/app.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, s-maxage=86400, max-age=0, stale-while-revalidate=86400" }
      ]
    },
    {
      "source": "/modules/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, s-maxage=86400, max-age=0, stale-while-revalidate=86400" }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=604800" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
